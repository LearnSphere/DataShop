    <class name="edu.cmu.pslc.learnsphere.analysis.moocdb.item.LongitudinalFeatureItem" table="longitudinal_features">
    <meta attribute="class-description">Represents a single longitudinal feature instance.</meta>
        <id name="id" type="int">
        <meta attribute="field-description">Database generated unique Id for this longitudinal feature.</meta>
            <column name="longitudinal_feature_id" />
            <generator class="native" />
        </id>
        <property name="name" type="string">
            <meta attribute="field-description">name.</meta>
            <column name="longitudinal_feature_name" />
        </property>
        <property name="description" type="string">
        <meta attribute="field-description">description.</meta>
            <column name="longitudinal_feature_description" />
        </property>
        
    </class>
</hibernate-mapping>
